{
  "private": true,
  "description": "React Material Components Web",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ofgeo/react.material.git"
  },
  "scripts": {
    "build": "cross-env BABEL_ENV=development webpack --progress --colors",
    "start:app": "cross-env NODE_ENV=production node app/server.js",
    "build:min": "cross-env BABEL_ENV=production webpack --config config/webpack.config.packages.js -p --progress --colors",
    "build:app": "node scripts/app.build.js",
    "clean": "del-cli packages/*/dist && lerna clean --yes",
    "dist": "yarn clean && yarn build && yarn build:min",
    "start": "cross-env BABEL_ENV=development NODE_ENV=development babel-node server-render.js",
    "serve": "cross-env BABEL_ENV=development NODE_ENV=development node server.js",
    "serve2": "cd ssr && cross-env BABEL_ENV=development NODE_ENV=development babel-node --presets env --plugins dynamic-import-node,transform-class-properties server2.js",
    "serve:build": "cd ssr && cross-env BABEL_ENV=development NODE_ENV=development webpack",
    "start:dev": "cross-env BABEL_ENV=development webpack --config config/webpack.config.packages.js --progress --colors --watch",
    "deploy": "node scripts/deploy.js",
    "!prepublish": "yarn dist"
  },
  "devDependencies": {
    "asset-module-webpack-plugin": "^0.0.2",
    "autoprefixer": "7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.2",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-remove-webpack": "^1.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.1.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.3.0",
    "cross-env": "^5.1.1",
