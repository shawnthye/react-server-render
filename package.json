    "file-loader": "1.1.5",
    "fs-extra": "4.0.2",
    "html-webpack-plugin": "2.30.1",
    "jest": "21.2.1",
    "jsdom": "^11.6.2",
    "lerna": "^2.5.1",
    "loadable-components": "^1.1.1",
    "memory-fs": "^0.4.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
